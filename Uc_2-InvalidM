package day_11;

import java.util.Scanner;

class Analyser 
{
	public String MoodAnalyser(String Mood)
	{
			if (null != Mood && ( Mood.equalsIgnoreCase("HAPPY") || Mood.equalsIgnoreCase("SAD"))) 
			{
				return Mood.toUpperCase();
			}
			else
			{
				return "HAPPY:";	
			}
		}
}

public class If_InvalidInput 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		Analyser a1 = new Analyser();
		System.out.println("Enter Mood:");
		String isMood = sc.next();
		String str = a1.MoodAnalyser(isMood);
		System.out.println("MOOD:"+str);
	}

}
